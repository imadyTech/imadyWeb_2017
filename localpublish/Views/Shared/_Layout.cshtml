    <script>
        var imadyweb = {
            showSpinner: function (text) {
                $('.spinner .text').html(text);
                $('.spinner').fadeIn();
                $('.body-content').css('opacity', 0);
            },

            hideSpinner: function () {
                $('.body-content').css('opacity', 1);
                $('.spinner .text').html('');
                $('.spinner').fadeOut();
            },

        };
        $(function () {
            imadyweb.hideSpinner();
        })
