<main class="page-content subpage_header">
    <section>
        <!--Swiper-->
        <div data-autoplay="5000" data-slide-effect="fade" data-loop="false" class="swiper-container swiper-slider">
            <div class="jumbotron text-center">
                <h1><small>@Resources.Resx_Home_Services.Sec_Header_PreTitle</small>@Resources.Resx_Home_Services.Sec_Header_Title</h1>
                <p class="big">@Resources.Resx_Home_Services.Sec_Header_Intro</p>
            </div>
            <div class="swiper-wrapper">
                <div data-slide-bg="/images/header-2.jpg" class="swiper-slide">
                    <div class="swiper-slide-caption"></div>
                </div>
            </div>
        </div>
    </section>


    <!-- 业务内容 Contents-->
    <section class="text-center text-sm-left well well-sm" id="Contents">
        <div class="container">
            <div class="row">
                <div class="col-lg-6 col-lg-offset-3 bg-white text-center">
                    <p class="lead">@Resources.Resx_Home_Services.Sec_Contents_Slogan</p>
                    <h1 class="text-bold">@Resources.Resx_Home_Services.Sec_Contents_Title</h1>
                    <p class="lead">@Resources.Resx_Home_Services.Sec_Contents_Introduction</p>
                </div>
            </div>
            <div class="row flow-offset-2">
                <div class="col-sm-4">
                    <span class="icon icon-xxl icon-primary fa-lightbulb-o"></span>
                    <h4 class="text-bold" id="Contents">@Resources.Resx_Home_Services.Sec_Contents_01_Name</h4>
                    <hr class="short bg-primary">
                    <p>@Resources.Resx_Home_Services.Sec_Contents_01_Content</p>
                </div>
                <div class="col-sm-4">
                    <span class="icon icon-xxl icon-primary fa-puzzle-piece"></span>
                    <h4 class="text-bold">@Resources.Resx_Home_Services.Sec_Contents_02_Name</h4>
                    <hr class="short bg-primary">
                    <p>@Resources.Resx_Home_Services.Sec_Contents_02_Content</p>
